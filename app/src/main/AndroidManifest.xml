<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prateek.secpass">

    <application
        android:allowBackup="true"
        android:icon="@drawable/secpass_notitle_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/secpass_notitle_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditNoteActivity"></activity>
        <activity android:name=".settingsattr.ChangeHintActivity" />
        <activity android:name=".settingsattr.ChangePassActivity" />
        <activity
            android:name=".drawerattr.SettingsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".drawerattr.TrashActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".drawerattr.TermsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".splash_welcome.LogInActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PasswordActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".splash_welcome.WelcomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>